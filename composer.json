{
    "name": "vm-labs/borica",
    "description": "Package that enables the communication with BORICA via their API",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "symfony/http-client": "^5.1",
        "symfony/form": "^4.4",
        "symfony/validator": "^4.4",
        "symfony/http-kernel": "^4.4",
        "symfony/config": "^4.4",
        "symfony/dependency-injection": "^4.4"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "Borica\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "Tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
